{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
		"exclude": ["../../App/tests", "../../App/views"],
		"include": [ "../../App" ],
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": [],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": true,
        "systemName": "remoteFileExplorer",
        "footer": "Created by Adam Barrell",
        "copyright": "2014",
        "navType": "vertical",
        "theme": "spacelab",
        "linenums": true,
        "collapseSymbols": true,
        "inverseNav": false
    },
	"opts": {
		"destination": "../../Docs",
		"template": "templates/default",
		"encoding": "utf8",
		"recurse": true,
		"query": "value",
		"private": true,
		"lenient": true
	}
}